#include <stdio.h>

int main(void)
{
    int a = 0;

    printf("Enter the number: ");

    if (scanf("%d", &a) != 1 || a <= 0)
    {
        printf("Invalid input\n");
        return 1;
    }

    for (int i = 1; i <= a; i++)
    {
        printf("%d ", i);
    }

    printf("\n");
    return 0;
}
